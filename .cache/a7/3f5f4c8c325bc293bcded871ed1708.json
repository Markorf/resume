 sans-serif;\n  background: $mainBgClr;\n  height: 100%;\n  width: 100%;\n  font-size: 1.15rem;\n  position: relative;\n  line-height: 35px;\n}\n\n#cvList {\n  list-style: none;\n  width: 80%;\n  margin: 4rem auto;\n  li {\n    display: flex;\n    color: $listClr;\n    counter-increment: custom;\n    padding-top: 0.8rem;\n    .colored {\n      color: $purple;\n      margin-left: 0.8rem;\n    }\n    &.functionClass {\n      .divForName {\n        color: lightblue;\n      }\n      .divForValue {\n        color: $purple;\n      }\n    }\n    &::before {\n      content: counter(custom) \" \";\n    }\n    &:first-child {\n      counter-reset: custom;\n    }\n\n    .divForName,\n    .divForValue {\n      display: inline-block;\n    }\n    .divForName {\n      color: $listClr;\n      margin-left: 2rem;\n    }\n    .divForValue {\n      margin-left: 1rem;\n      color: $valuesClr;\n      a {\n        color: inherit;\n      }\n    }\n\n    &:hover {\n      background: lighten($mainBgClr, 20%);\n    }\n  }\n}\n\nfooter.main-footer {\n  font-size: 1rem;\n  background: darken($mainBgClr, 3%);\n  position: fixed;\n  bottom: 0;\n  display: flex;\n  flex-direction: column;\n  padding: 1rem;\n  left: 0;\n  right: 0;\n  color: $valuesClr;\n  justify-content: center;\n  transition: all 0.5s ease-in;\n\n  &.hidden {\n    opacity: 0;\n  }\n\n  .contact {\n    transition: all 0.5s ease-out;\n    display: flex;\n\n    &.invisible {\n      display: none;\n      opacity: 0;\n    }\n\n    & .close-icon {\n      cursor: pointer;\n      background: white;\n      position: absolute;\n      right: 10px;\n    }\n\n    .location {\n      flex-basis: 33%;\n      text-align: center;\n      h2 {\n        margin-bottom: 1rem;\n      }\n    }\n    #map {\n      height: 74%;\n    }\n    .formDiv {\n      flex-basis: 65%;\n      input,\n      label,\n      button {\n        display: block;\n      }\n      input,\n      textarea {\n        padding: 0.5rem;\n        width: 80%;\n      }\n      textarea {\n        resize: none;\n        height: 100px;\n      }\n      button {\n        padding: 0.3rem 2rem;\n        cursor: pointer;\n        background: $purple;\n        border: none;\n        border-radius: 5px;\n        color: white;\n        &:hover {\n          background: lighten($purple, 10%);\n        }\n      }\n    } // kraj za formDiv\n  } // kraj za contact\n\n  .about {\n    position: relative;\n    transition: all 0.5s ease-out;\n    &.invisible {\n      display: none;\n      opacity: 0;\n    }\n    & p {\n      width: 95%;\n    }\n    & .close-icon {\n      cursor: pointer;\n      background: white;\n      position: absolute;\n      right: 0;\n    }\n  }\n} // kraj za footer\n\n@media screen and (max-width: 940px) {\n  #cvList {\n    width: 100%;\n  }\n  .main-footer {\n    position: relative !important;\n    &.hidden {\n      display: none;\n    }\n    .contact {\n      flex-direction: column;\n      .location {\n        width: 90%;\n        margin: 2rem 0;\n        #map {\n          padding: 5rem;\n        }\n      }\n      .formDiv {\n        flex-basis: 100%;\n        input,\n        textarea {\n          width: 90% !important;\n        }\n      }\n    }\n  }\n}\n\n@media screen and (max-width: 750px) {\n  html,\n  body {\n    font-size: 1.1rem;\n  }\n}\n\n@media screen and (max-width: 550px) {\n  html,\n  body {\n    font-size: 1rem;\n    #cvList {\n      li {\n        margin-left: -1.5rem;\n        .divForName {\n          margin-left: 0.5rem;\n        }\n      }\n    }\n  }\n}\n\n@media screen and (max-width: 380px) {\n  html,\n  body {\n    font-size: 0.93rem;\n    font-weight: bolder;\n  }\n  #cvList {\n    li {\n      margin-left: -2.3rem;\n      .divForValue {\n        margin-left: 0;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"6ad8b6f19185c82ec5acded66034ddab","cacheData":{"env":{}}}